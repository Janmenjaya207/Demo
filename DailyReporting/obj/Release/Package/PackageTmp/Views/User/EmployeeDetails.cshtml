                                                        @Html.DropDownListFor(x => x.tasDetails.Agreed_For_Training, prio, new { @class = "form-control form-control1", placeholder = "", @id = "Agreed_For_Training", @name = "Agreed_For_Training", @onchange = "trIndent()" })
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label class="form-label" for="example-select">Option of field where training intended:</label>
                                                        @*@Html.DropDownListFor(m => m.tasDetails.Tr_Intended_Id, Model.TraingIntend, new { @required = "true", @class = "form-control ", @id = "TraingIntend", })*@
                                                        @Html.TextBoxFor(m => m.tasDetails.Tr_Intended_details, new { @class = "form-control ", @id = "TraingIntend", @name = "TraingIntend" })
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label class="form-label" for="example-select">Remarks:</label>
                                                        @Html.TextAreaFor(x => x.tasDetails.Remarks, new { @class = "form-control", @name = "Remarks", @id = "Remarks" })
                                                    </div>
                                                </div>
                                            </div>


                                            
                                        



                                            <div class="row mt-4">
                                                <div class="col-md-12">
                                                    <button type="button" class="btn btn-warning waves-effect waves-themed" style="float:right;"onclick="location.href='@Url.Action("EmployeeDetails", "User")';">Cancel</button>
