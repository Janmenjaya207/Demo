<script>
   var inc = "";
    var inc1 = "";
    var inc2 = "";
    $(document).ready(function () {
        debugger;

        inc = '@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.traininginstitute))';
        inc = JSON.parse(inc);
         inc1 = '@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.trainingType))';

        inc1 = JSON.parse(inc1);
         inc2 = '@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.year))';

        inc2 = JSON.parse(inc2);
    })
    var count = 0;
    var sum = 0;
    function AddMore1() {
        debugger;
        if ($("#Institution" + count).val() == 0) {
            alert("Please Select Institute ");
            return false;
        }
        else if ($("#trainingType" + count).val() == 0) {
            alert("Please Select Training Type ");
            return false;
        }
        else if ($("#fdate" + count).val() == "") {
            alert("Please Enter Year");
            return false;
        }
        else if ($("#tdate" + count).val() == "") {
            alert("Please Enter Days");
            return false;
        }
        else {
            count = count + 1;

            var data = "";
            data += '<tr id="tr_' + count + '">';
            data += '<td><select name="Institution" id="Institution' + count + '" class="form-control " required onchange="BindJhut()"></select></td>';
            data += '<td><select name="trainingType" id="trainingType' + count + '" class="form-control " required></select></td>';
            data += '<td><select name="fdate" id="fdate' + count + '" class="form-control " required></select></td>';

            data += '<td><input type="number" class="form-control item_Total" name="tdate" id="tdate'+ count +'" /></td>';


            data += '<td><button onclick="Remove1(' + count + ')" class="btn btn-outline-primary">X</button></td>';
            data += '</tr>';

            $("#addsupply").append(data);
            loaddrp(count);
        }
    }
    function loaddrp(count) {
        debugger;
        var list = "";
        var list1 = "";
        var list2 = "";

        if (inc.length > 0) {
            for (var i = 0; i < inc.length; i++) {
                list += "<option value=" + inc[i].Value + ">" + inc[i].Text + "</option>";
            }
            $("#Institution" + count).html(list);
        }
        if (inc1.length > 0) {
            for (var i = 0; i < inc1.length; i++) {
                list1 += "<option value=" + inc1[i].Value + ">" + inc1[i].Text + "</option>";
            }
            $("#trainingType" + count).html(list1);
        }
        if (inc2.length > 0) {
            for (var i = 0; i < inc2.length; i++) {
                list2 += "<option value=" + inc2[i].Value + ">" + inc2[i].Text + "</option>";
            }
            $("#fdate" + count).html(list2);
        }

    }
    function Remove1(id)

    {
        $("#tr_" + id).remove();
    }


    function BindJhut() {
        var state = $('#Institution').val();
        var statecount = $('#Institution' + count).val();
        if (count == 0) {
            $.ajax({
                type: "post",
                url: "/USer/Traintype",
                data: { cccc: state },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var district = "<select id='pblock'class='form-control'>";
                    district = district + '<option value="">--Select--</option>';
                    for (var i = 0; i < data.length; i++) {
                        district = district + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                    district = district + '</select>';

                    if (count == 0) {
                        $("#trainingType").html(district);
                    }
                    else {
                        $("#trainingType" + count).html(district);
                    }
                }
            });
        }
        else {
            $.ajax({
                type: "post",
                url: "/User/Traintype",
                data: { cccc: statecount },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var district = "<select id='pblock'class='form-control'>";
                    district = district + '<option value="">--Select--</option>';
                    for (var i = 0; i < data.length; i++) {
                        district = district + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
                    }
                    district = district + '</select>';

                    if (count == 0) {
                        $("#trainingType").html(district);
                    }
                    else {
                        $("#trainingType" + count).html(district);
                    }
                }
            });
        }
    }
</script>

<script type="text/javascript">
    $("#btnSubmit").click(function () {
        debugger;
        var count = 0;

        var Emp_Name = $("#Emp_Name").val();
        var Email = $("#Email").val();
        var designation = $("#designation").val();
        var Present_Post = $("#Present_Post").val();
        var Edu_Qualification = $("#Edu_Qualification").val();
        var Retirement_Date = $("#Retirement_Date").val();

        if (Emp_Name == "") {
            $('#Emp_Name').next("label").remove();
            $('#Emp_Name').after('<label class="error mt-2 text-danger1 ">Please Enter Employee Name</label>');
            $("#Emp_Name").focus();
            count = 1;
            return false;
        }
        else {
            $('#Emp_Name').next("label").remove();
        }

        if (Email == "") {
            $('#Email').next("label").remove();
            $('#Email').after('<label class="error mt-2 text-danger1">Please Enter Employee Email Id</label>');
            $("#Email").focus();
            count = 1;
            return false;
        }
        else {
            if (ValidateEmail(Email)) {
                $("#Email").next("label").remove();
            }
            else {
                $("#Email").next("label").remove();
                $("#Email").after('<label class="error mt-2 text-danger1">Please Enter Valid Email Id</label>');
                $("#Email").focus();
                count = 1;
                return false;
            }
        }
        if (designation == 0) {
            $('#designation').next("label").remove();
